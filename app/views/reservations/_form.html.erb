<%= form_for [property, reservation] do |f| %>
  <div>
    <%= f.label :start %>
    <%= f.date_field :start %>
  </div>

  <div>
    <%= f.label :end %>
    <%= f.date_field :end %>
  </div>

  <div>
    <%= f.submit "Book Stay", class: "button button-secondary button-submit" %>
  </div>

<% end %>